exports.version = (function Version(bot, doc, user, userID, channelID, message, event) {
    if(message === undefined){
        return;
    }
    if (message === "*ver" || message === "*version") {
        bot.sendMessage({
            to: doc.logchannel,
            message: doc.versionevent
        });
        bot.sendMessage({
            to: channelID,
            message: doc.versionreply
        });
    }
});
